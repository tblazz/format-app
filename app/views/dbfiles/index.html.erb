<p id="notice"><%= notice %></p>

<h1>Dbfiles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dbfiles.each do |dbfile| %>
      <tr>
        <td><%= link_to 'Show', dbfile %></td>
        <td><%= link_to 'Edit', edit_dbfile_path(dbfile) %></td>
        <td><%= link_to 'Destroy', dbfile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dbfile', new_dbfile_path %>
